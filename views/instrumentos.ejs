<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Todo Música</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="../css/instrumentos.css" />
    <link rel="stylesheet" href="./css/footer_nav.css">
</head>

<body>
    <%- include('partials/nav'); %>
    <%- include('partials/nav-logout'); %>
    <div id="container">
        <div id="h1container">
            <h1 id="categorias">Nuestras categorías</h1>
            <button id="carritoIni" class="btn btn-dark" data-bs-toggle="offcanvas" data-bs-target="#offcanvasRight"
                aria-controls="offcanvasRight"><img class="foto_carrito" src="http://127.0.0.1:3000/img/carrito.png"></button>
        </div>
        <div id="instrumentos">
            <button id="guitarras_electricas" class="instrumentos"><img
                    src="https://th.static-thomann.de/thumb//thumb250x250/pics/prod/124838.jpg">
                <p>Guitarras eléctricas</p>
            </button>
            <button id="guitarras_clasicas" class="instrumentos"> <img
                    src="https://th.static-thomann.de/thumb//thumb250x250/pics/prod/130179.jpg">
                <p>Guitarras clásicas</p>
            </button>
            <button id="bajos_electricos" class="instrumentos"> <img
                    src="https://th.static-thomann.de/thumb//thumb250x250/pics/prod/264956.jpg">
                <p>Bajos eléctricos</p>
            </button>
            <button id="instrumentos_de_teclas" class="instrumentos"> <img
                    src="https://th.static-thomann.de/thumb//thumb250x250/pics/prod/352258.jpg">
                <p>Instrumentos de teclas</p>
            </button>
            <button id="instrumentos_de_arco" class="instrumentos"> <img
                    src="https://th.static-thomann.de/thumb//thumb250x250/pics/prod/165069.jpg">
                <p>Instrumentos de arco</p>
            </button>
            <button id="saxofones" class="instrumentos"> <img
                    src="https://th.static-thomann.de/thumb//thumb250x250/pics/prod/188679.jpg">
                <p>Saxofones</p>
            </button>
            <button id="trompetas" class="instrumentos"> <img
                    src="https://th.static-thomann.de/thumb//thumb250x250/pics/prod/152571.jpg">
                <p>Trompetas</p>
            </button>
            <button id="trombones" class="instrumentos"> <img
                    src="https://th.static-thomann.de/thumb//thumb250x250/pics/prod/288562.jpg">
                <p>Trombones</p>
            </button>
            <button id="amplificadores_de_guitarra_electrica" class="instrumentos"> <img
                    src="https://th.static-thomann.de/thumb//thumb250x250/pics/prod/429320.jpg">
                <p>Amplificadores de <br> guitarra eléctrica</p>
            </button>
            <button id="drums_acusticos" class="instrumentos"> <img
                    src="https://th.static-thomann.de/thumb//thumb250x250/pics/prod/327902.jpg">
                <p>Baterías acústicas</p>
            </button>
        </div>
        <div id="contenedor_instrumentos">

        </div>
    </div>
    <div class="offcanvas offcanvas-end" tabindex="-1" id="offcanvasRight" aria-labelledby="offcanvasRightLabel">
        <div class="offcanvas-header">
            <h5 id="offcanvasRightLabel">Carrito</h5>
            <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>
        <div class="offcanvas-body">

        </div>
    </div>    
    <%- include('partials/footer'); %>
    <script src="./scripts/logout.js"></script>
    <script src="./scripts/instrumentos.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.10.2/dist/umd/popper.min.js"
        integrity="sha384-7+zCNj/IqJ95wo16oMtfsKbZ9ccEh31eOz1HGyDuCQ6wgnyJNSYdrPa03rtR1zdB"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js"
        integrity="sha384-QJHtvGhmr9XOIpI6YVutG+2QOK9T+ZnN4kzFN1RtK3zEFEIsxhlmWl5/YESvpZ13"
        crossorigin="anonymous"></script>
</body>

</html>